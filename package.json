{
  "name": "adaptic-backend",
  "version": "1.0.132",
  "description": "A highly scalable backend solution using Prisma, Apollo Server, and TypeGraphQL, featuring GraphQL and RESTful APIs with real-time subscriptions and robust authentication.",
  "main": "dist/server.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "dist/node_modules/",
    "README.md",
    "package.json",
    "server.ts",
    "client.ts"
  ],
  "scripts": {
    "clean": "rm -rf dist && rm -rf src/generated",
    "dev": "npm run migrate:dev  && npm run fix-imports && nodemon --exec ts-node src/server.ts",
    "build": "npm run clean && npm run generate && npm run fix-imports && npm run generate:functions && tsc && npm run build:server",
    "build:server": "tsc --project tsconfig.server.json",
    "build:package": "npm run build && npm run prepare-package-folder",
    "start": "npm run migrate && node dist/server.js",
    "generate": "npx prisma generate",
    "migrate:dev": "npx prisma migrate dev && npm run migrate",
    "migrate": "npx prisma migrate deploy",
    "update-backend-functions": "node update-backend-functions.mjs",
    "generate:functions": "ts-node src/modules/index.ts",
    "fix-imports": "node fix-import-paths.cjs",
    "prepare-package-folder": "node prepare-package.mjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@apollo/client": "^3.11.8",
    "@babel/cli": "^7.25.7",
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/graphql-fields": "^1.3.9",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/next-auth": "^3.13.0",
    "@types/node": "^20.14.11",
    "@types/pluralize": "^0.0.33",
    "@types/ws": "^8.5.11",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typegraphql-prisma": "^0.27.2",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@aws-sdk/client-lambda": "^3.624.0",
    "@prisma/client": "^5.17.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "graphql": "^16.9.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.23.0",
    "graphql-ws": "^5.16.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "next-auth": "^4.24.8",
    "pluralize": "^8.0.0",
    "prisma": "^5.17.0",
    "railway": "^2.0.17",
    "react": "latest",
    "react-dom": "latest",
    "redis": "^4.6.15",
    "reflect-metadata": "^0.2.2",
    "type-graphql": "^2.0.0-rc.2",
    "ws": "^8.18.0"
  }
}
