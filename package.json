{
  "name": "adaptic-backend",
  "version": "1.0.0",
  "description": "A highly scalable backend solution using Prisma, Apollo Server, and TypeGraphQL, featuring GraphQL and RESTful APIs with real-time subscriptions and robust authentication.",
  "main": "dist/server.js",
  "types": "dist/types/index.d.ts",
  "module": "dist/types/index.mjs",
  "files": [
    "dist/types/",
    "dist/package.json",
    "dist/README.md",
    "dist/generated/",
    "dist/node_modules/",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "clean": "rm -rf dist && rm -rf src/generated",
    "dev": "npm run migrate:dev  && npm run fix-imports && nodemon --exec ts-node src/server.ts",
    "build": "npm run clean && npm run generate && npm run fix-imports && tsc",
    "build:package": "npm run build && npm run prepare-package-folder",
    "start": "npm run migrate && node dist/server.js",
    "generate": "npx prisma generate",
    "migrate:dev": "npx prisma migrate dev && npm run migrate",
    "migrate": "npx prisma migrate deploy",
    "update-backend-functions": "node update-backend-functions.mjs",
    "fix-imports": "node fix-import-paths.js",
    "prepare-package-folder": "node prepare-package.mjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/graphql-fields": "^1.3.9",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.11",
    "@types/ws": "^8.5.11",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.2",
    "typegraphql-prisma": "^0.27.2",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@aws-sdk/client-lambda": "^3.624.0",
    "@prisma/client": "^5.17.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "graphql": "^16.9.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.23.0",
    "graphql-ws": "^5.16.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.17.0",
    "railway": "^2.0.17",
    "redis": "^4.6.15",
    "reflect-metadata": "^0.2.2",
    "type-graphql": "^2.0.0-rc.2",
    "ws": "^8.18.0"
  }
}
